          SQLTask:SqlStatementSource="INSERT INTO STG_FactInternetSales (CustomerID, EmployeeID, OrderDate, ShipperID, ProductID, SupplierID, UnitPrice, Quantity)&#xA;SELECT &#xA;    o.CustomerID,&#xA;    o.EmployeeID,&#xA;    o.OrderDate,&#xA;    o.ShipVia AS ShipperID,&#xA;    od.ProductID,&#xA;    p.SupplierID,&#xA;    od.UnitPrice,&#xA;    od.Quantity&#xA;FROM Orders o&#xA;JOIN OrderDetails od ON o.OrderID = od.OrderID&#xA;JOIN Products p ON od.ProductID = p.ProductID;&#xA;&#xA;&#xA;DELETE FROM STG_FactInternetSales&#xA;WHERE CustomerID IS NULL&#xA;   OR EmployeeID IS NULL&#xA;   OR ProductID IS NULL&#xA;   OR OrderDate IS NULL;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
