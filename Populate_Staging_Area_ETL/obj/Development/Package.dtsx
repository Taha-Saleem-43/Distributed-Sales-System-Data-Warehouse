      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{4D9AF75F-9E36-4991-961F-BA84D2A2CD14}"
          SQLTask:SqlStatementSource="DECLARE @StartDate DATE;&#xA;DECLARE @EndDate   DATE;&#xA;&#xA;-- 1. Get min and max OrderDate automatically&#xA;SELECT&#xA;    @StartDate = MIN(OrderDate),&#xA;    @EndDate   = MAX(OrderDate)&#xA;FROM Orders;   -- staging or DW orders source&#xA;&#xA;-- Safety check&#xA;IF @StartDate IS NULL OR @EndDate IS NULL&#xA;BEGIN&#xA;    RAISERROR ('OrderDate range not found.', 16, 1);&#xA;    RETURN;&#xA;END;&#xA;&#xA;-- 2. Generate Time dimension&#xA;WHILE @StartDate &lt;= @EndDate&#xA;BEGIN&#xA;    INSERT INTO STG_Time (&#xA;        TimeKey,&#xA;        [Date],&#xA;        DayNbWeek,&#xA;        DayNbMonth,&#xA;        DayNbYear,&#xA;        MonthName,&#xA;        Quarter,&#xA;        [Year]&#xA;    )&#xA;    VALUES (&#xA;        YEAR(@StartDate) * 10000&#xA;        + MONTH(@StartDate) * 100&#xA;        + DAY(@StartDate),&#xA;&#xA;        @StartDate,&#xA;        DATEPART(WEEKDAY, @StartDate),&#xA;        DAY(@StartDate),&#xA;        DATEPART(DAYOFYEAR, @StartDate),&#xA;        DATENAME(MONTH, @StartDate),&#xA;        DATEPART(QUARTER, @StartDate),&#xA;        YEAR(@StartDate)&#xA;    );&#xA;&#xA;    SET @StartDate = DATEADD(DAY, 1, @StartDate);&#xA;END;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
